{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 0,
    "no-plusplus": "off",
    "no-loop-func": "warn",
    "no-unused-expressions": "warn",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
      }
    ]
  }
}
